<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <system.web>
    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.6.1" />
  </system.web>
  <connectionStrings>
    <add name="BdContext" connectionString="Data Source=(LocalDb)\MSSQLLocalDB;Initial Catalog=cgfApp;Integrated Security=SSPI;" providerName="System.Data.SqlClient"/>
